
<%- include("partials/header"); -%>
    <h1 class="compose">COMPOSE</h1>
    <form action="/compose" method="post">
      <div class="form-group">
        <label>TITLE</label>
        <input class="form-control" type="text" name="postTitle" autocomplete="off">
        <label>POST</label>
        <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
      </div>
      <button class="btn btn-lg" type="submit" name="button" id="button-start-4">POST
        <i class="fa-solid fa-2xl fa-check" id="check"></i>
      </button>
    </form>

<%- include("partials/footer"); -%>
